import random

# 定义一个包含多种美食的列表
foods = [
    "披萨",
    "汉堡",
    "寿司",
    "拉面",
    "炸鸡",
    "牛排",
    "炒饭",
    "饺子",
    "咖喱饭",
    "意大利面",
    "烤鸭",
    "烤肉",
    "火锅",
    "麻辣烫",
    "煎饼果子",
    "烤鱼",
    "寿喜烧",
    "三明治",
    "沙拉",
    "冰淇淋"
]

# 随机选择一种食物
random_food = random.choice(foods)

# 输出结果
print(f"今天你可以吃：{random_food}")
